{
  "name": "react-routes",
  "version": "1.0.0",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "description": "React Router Spike",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "dev:hot": "./node_modules/.bin/webpack-dev-server --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./public",
	"document:test:suite:functional:steps": "./node_modules/.bin/jsdoc -c ./configuration/documentation/tests/functional/suite/jsdoc-steps.json",
	"document:test:suite:functional:support": "./node_modules/.bin/jsdoc -c ./configuration/documentation/tests/functional/suite/jsdoc-support.json",
	"document:test:results:functional": "./node_modules/.bin/allure generate tests/functional/reports/ -o documentation/tests/functional/results",
    "postinstall": "./node_modules/.bin/selenium-standalone install",
    "start": "nodemon server.js",
    "test:unit": "./node_modules/.bin/mocha tests/unit/helpers/browser.js tests/unit/specs/*.spec.js",
    "test:functional": "./node_modules/.bin/wdio webdriverio.js",
    "watch": "./node_modules/.bin/webpack -d --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.0",
    "css-loader": "^0.25.0",
    "cucumber": "^1.3.1",
    "docdash": "^0.4.0",
    "enzyme": "^2.5.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdoc": "^3.4.2",
    "jsdoc-jsx": "^0.1.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "postcss": "^5.2.5",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-reporter": "^1.4.1",
    "postcss-url": "^5.1.2",
    "react-addons-test-utils": "^15.3.2",
    "selenium-standalone": "^5.8.0",
    "style-loader": "^0.13.1",
    "wdio-allure-reporter": "^0.1.2",
    "wdio-cucumber-framework": "^0.2.11",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "0.0.7",
    "wdio-spec-reporter": "0.0.3",
    "webdriverio": "^4.4.0",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "babel-polyfill": "^6.16.0",
    "d3": "^4.3.0",
    "express": "^4.14.0",
    "jquery": "^3.1.0",
    "object-assign": "^4.1.0",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
